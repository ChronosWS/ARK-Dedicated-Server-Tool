                                                    <Path Data="F1M13.131,1.313L0,1.313 0,0 13.131,0z" Fill="White" Margin="44.618,41.077,9.251,11.61"/>
                                                </Grid>
                                            </Viewbox>
                                        </Button>
                                        <Button x:Name="openRCONButton_Copy"  ToolTip="Open RCON" HorizontalAlignment="Right" Width="40" Margin="4,0,0,0" Height="40">
                                            <Grid Height="30" Width="30">
                                                <Viewbox Stretch="Fill" Margin="4,2,6,12">
                                                    <Grid Height="54" Width="67">
                                                        <Path Data="F1M65,10L2,10 2,2 65,2z M0,54L67,54 67,0 0,0z" Fill="Black"/>
                                                        <Path Data="F1M13.395,17.438C12.58,17.86 10.874,18.279 8.72,18.279 3.73,18.279 0,15.126 0,9.297 0,3.73 3.755,0 9.245,0 11.423,0 12.843,0.473 13.446,0.79L12.869,2.654C12.029,2.233 10.794,1.918 9.324,1.918 5.175,1.918 2.416,4.57 2.416,9.218 2.416,13.579 4.913,16.334 9.193,16.334 10.611,16.334 12.029,16.046 12.948,15.599z" Fill="White" Margin="10.356,21.118,43.198,14.603"/>
