                            <GroupBox FontFamily="{DynamicResource HeaderFont}" FontSize="16" Foreground="{DynamicResource TextLight_Brush}" d:LayoutOverrides="LeftPosition, RightPosition" Margin="6,0,6,10">
                                <GroupBox.Header>
                                    <CheckBox x:Name="enableSotFCB" IsChecked="{Binding Profile.SOTF_Enabled}" Content="Survival of the Fittest" FontFamily="{StaticResource GroupBoxHeaderFont}" FontSize="{StaticResource GroupBoxHeaderFontSize}" Foreground="{StaticResource TextLight_Brush}" Height="25"  />
                                </GroupBox.Header>
                                <Grid IsEnabled="{Binding IsChecked, ElementName=enableSotFCB}">
                                    <CheckBox Content="Disable Death Spectator" IsChecked="{Binding Profile.SOTF_DisableDeathSPectator}" Foreground="{StaticResource TextLight_Brush}" Margin="10,10,0,0" FontFamily="{StaticResource DetailBody_Font}" FontSize="{StaticResource DetailBodyFont_Size}" Height="22" ToolTip="If checked, players cannot be spectators after they die." HorizontalAlignment="Left" VerticalAlignment="Top" d:LayoutOverrides="Height"/>
                                    <CheckBox Content="Only Admin Rejoin as Spectator" IsChecked="{Binding Profile.SOTF_OnlyAdminRejoinAsSpectator}" Foreground="{StaticResource TextLight_Brush}" Margin="287,10,121,0" FontFamily="{StaticResource DetailBody_Font}" FontSize="{StaticResource DetailBodyFont_Size}" Height="22" ToolTip="If checked, only admins can join as spectators once the game has started." VerticalAlignment="Top" d:LayoutOverrides="Height"/>
                                    <UniformGrid Rows="2" Columns="3" Height="140" Margin="6,37,10,23">
                                        <local:LabeledSlider LabelText="Max Players in Tribe:" Value="{Binding SOTF_MaxNumberOfPlayersInTribe}" Minimum="1" Margin="4" Maximum="127" SmallChange="5" LargeChange="20" ToolTip="The maximum number of players allowed in a tribe."/>
                                        <local:LabeledSlider LabelText="Tribes Required for Game:" Value="{Binding SOTF_BattleNumOfTribesToStartGame}"  Minimum="1" Margin="4" Maximum="127" SmallChange="5" LargeChange="20" ToolTip="The number of tribes required for the game to start."/>
                                        <local:LabeledSlider LabelText="Ring of Death Collapse Time (sec):" Value="{Binding SOTF_TimeToCollapseROD}"  Minimum="60" Margin="4" Maximum="86400" SmallChange="900" LargeChange="3600" ToolTip="The game length before Sudden Death."/>
                                        <local:LabeledSlider LabelText="Auto-start Delay Time (sec):" Value="{Binding SOTF_BattleAutoStartGameInterval}" Minimum="5" Margin="4" Maximum="120" SmallChange="5" LargeChange="20" ToolTip="The time after the minimum number of tribes has been achieved before the game starts."/>
                                        <local:LabeledSlider LabelText="Auto-restart Delay Time (sec):" Value="{Binding SOTF_BattleAutoRestartGameInterval}"  Minimum="5" Margin="4" Maximum="120" SmallChange="5" LargeChange="20" ToolTip="The time after the last game has ended before the new game will start."/>
                                        <local:LabeledSlider LabelText="Sudden Death Time (sec):" Value="{Binding SOTF_BattleSuddenDeathInterval}"  Minimum="60" Maximum="3600" Margin="4" SmallChange="60" LargeChange="600" ToolTip="The amount of time Sudden Death will be in effect until the game is declared a draw."/>
                                    </UniformGrid>
                                </Grid>

                            </GroupBox>
                        </StackPanel>
                    </ScrollViewer>
                </Grid>

            </TabItem>
            <TextBox x:Name="textBox" Height="79" TextWrapping="Wrap" Text="TextBox" Width="260"/>

        </TabControl>
        <Border x:Name="border" d:LayoutOverrides="Height, LeftMargin, RightMargin" Background="{StaticResource DarkNeutralFade_Brush}" BorderBrush="Black" BorderThickness="1" RenderTransformOrigin="0.5,0.5" Margin="0,0,0,-2.664">
            <Border.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Border.RenderTransform>
            <StackPanel Orientation="Vertical">
                <ListBox x:Name="listBox" MinWidth="200" Style="{StaticResource MainListBoxStyle}" FontFamily="/ARK Server Manager;component/Fonts/#Montserrat" FontSize="18.667" Margin="0" Background="{x:Null}" Foreground="{StaticResource TextLight_Brush}" BorderBrush="{x:Null}" d:LayoutOverrides="LeftMargin, RightMargin" VerticalAlignment="Top" Loaded="listBox_Loaded" HorizontalContentAlignment="Stretch" HorizontalAlignment="Center" d:IsLocked="True">
                    <ListBoxItem x:Name="listBoxItemAdmin" Content="Administration" Style="{StaticResource MainListBoxItem}" Padding="0,8,0,0" Margin="0,3,0,0" HorizontalContentAlignment="Center"/>
                    <ListBoxItem x:Name="listBoxItemRules" Content="Rules" Style="{StaticResource MainListBoxItem}" Padding="4,8"/>
                    <ListBoxItem x:Name="listBoxItemUI" Content="UI" Style="{StaticResource MainListBoxItem}" Padding="4,8"/>
                    <ListBoxItem x:Name="listBoxItemPlayers" Content="Player Settings" Style="{StaticResource MainListBoxItem}" Padding="4,8"/>
                    <ListBoxItem x:Name="listBoxItemDinos" Content="Dino Settings" Style="{StaticResource MainListBoxItem}" Padding="4,8"/>
                    <ListBoxItem x:Name="listBoxItemStructures" Content="Structures" Style="{StaticResource MainListBoxItem}"/>
                    <ListBoxItem x:Name="listBoxItemEnviron" Content="Environment" Style="{StaticResource MainListBoxItem}" Padding="4,8"/>
                    <ListBoxItem x:Name="listBoxItemMods" Content="Mods" Style="{StaticResource MainListBoxItem}" Padding="4,8"/>
                </ListBox>
                <Button x:Name="backToListButton" Height="42.837" Margin="0,25,0,0" BorderBrush="Black" HorizontalAlignment="Center" Width="154" BorderThickness="2" FontFamily="{StaticResource ButtonFont}" FontSize="16" Background="White" Foreground="White" ToolTip="Return to server list">
                    <StackPanel Orientation="Horizontal" Margin="0,0,30,0">
                        <Path Data="F1M0,0L76.591,76.595 153.188,0 76.591,38.297z" Fill="{StaticResource ButtonText_Brush}" Stretch="Fill" Width="32.194" RenderTransformOrigin="0.5,0.5" Height="16.097">
                            <Path.RenderTransform>
                                <TransformGroup>
                                    <ScaleTransform/>
                                    <SkewTransform/>
                                    <RotateTransform Angle="90"/>
                                    <TranslateTransform/>
                                </TransformGroup>
                            </Path.RenderTransform>
                        </Path>
                        <TextBlock x:Name="textBlock7" TextWrapping="Wrap" Margin="0" VerticalAlignment="Center" Text="List" FontSize="24" FontFamily="{StaticResource DefaultFont}" d:LayoutOverrides="TopMargin, BottomMargin, LeftPosition, RightPosition"/>
                    </StackPanel>
                </Button>
            </StackPanel>
        </Border>
    </Grid>
</UserControl>
