                                                        <Path Data="F1M7.142,19.039L0,0 1.787,0 8.928,19.039z" Fill="White" Margin="30.141,21.093,27.931,13.868"/>
                                                        <Path Data="F1M13.131,1.313L0,1.313 0,0 13.131,0z" Fill="White" Margin="44.618,41.077,9.251,11.61"/>
                                                    </Grid>
                                                </Viewbox>
                                                <TextBlock x:Name="textBlock2" Margin="1.734,0,1.266,0.971" TextWrapping="Wrap" Text="RCON" VerticalAlignment="Bottom" FontSize="9.333" FontWeight="Bold" Height="11" Foreground="{StaticResource ButtonText_Brush}"/>
                                            </Grid>
                                        </Button>
                                    </StackPanel>
                                </Border>
                            </Grid>
                            <GroupBox Header="Profile Basics" FontFamily="{StaticResource HeaderFont}" FontSize="16" Foreground="{StaticResource TextLight_Brush}" d:LayoutOverrides="LeftPosition, RightPosition" Margin="6,0,6,10">
                                <UniformGrid Rows="2" Columns="2" Height="140">
                                    <local:LabeledTextBox x:Name="ltbProfileName" Height="60" InputText="{Binding Profile.ProfileName}" LabelText="Server Profile Name:" Width="Auto" d:LayoutOverrides="TopPosition, BottomPosition" Margin="{StaticResource DetailItemMargins}" RenderTransformOrigin="0.502,0.467" />
                                    <Grid Height="60" Background="#00000000" Margin="{StaticResource DetailItemMargins}" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
                                        <local:LabeledTextBox Height="60" InputText="{Binding Profile.InstallDirectory}" LabelText="Installation Location:" Width="Auto" />
                                        <Button x:Name="installFolderButton"  ToolTip="Change install location" HorizontalAlignment="Right" Height="22" VerticalAlignment="Top" Width="22" Margin="0,3,4,0">
                                            <Viewbox Height="13" Stretch="Fill" Width="16">
                                                <Grid Height="45" Width="58">
                                                    <Path Data="F1M0,8.126C0,8.126 0.103,5.94 1.104,5.704 2.107,5.468 2.511,4.629 2.629,3.038 2.747,1.448 3.799,0 4.626,0L20.005,0C20.005,0 21.831,1.027 21.949,2.736 22.067,4.445 22.421,5.531 24.249,5.531L49.231,5.531C49.231,5.531,51.708,5.704,52.061,7.649z" Fill="{StaticResource ButtonText_Brush}" Margin="2.645,0,2.96,36.943"/>
                                                    <Path Data="F1M31.988,31.177L27.134,31.177 31.731,23.177 13.76,23.177 13.76,14.177 31.731,14.177 27.134,6.177 31.988,6.177 44.305,18.677z M54.706,0.177L2.851,0.177C-0.449,0.177 0.023,2.4 0.023,2.4 0.023,6.76 1.201,30.348 1.201,32.469 1.201,34.59 2.851,35.177 2.851,35.177L54.94,35.177C56.473,35.177 56.473,32.177 56.473,32.177 56.473,23.103 57.418,5.888 57.653,2.47 57.889,-0.948 54.706,0.177 54.706,0.177" Fill="{StaticResource ButtonText_Brush}" Margin="0,9.892,0,0"/>
                                                </Grid>
                                            </Viewbox>
                                        </Button>
                                    </Grid>
                                    <local:LabeledTextBox Height="60" InputText="{Binding Profile.AdditionalArgs}" LabelText="Additional Args:" Width="Auto" d:LayoutOverrides="TopPosition, BottomPosition" Margin="{StaticResource DetailItemMargins}" RenderTransformOrigin="0.502,0.467" />
                                </UniformGrid>
                            </GroupBox>
                            <GroupBox Header="Server Details" Margin="6,0,6,10"  FontFamily="{StaticResource HeaderFont}" FontSize="16" Foreground="{StaticResource TextLight_Brush}" d:LayoutOverrides="TopPosition, BottomPosition">
                                <UniformGrid Rows="4" Columns="3" Height="280">
                                    <Controls:LabeledTextBlock BodyText="{Binding Profile.Version, FallbackValue=[Runtime Value]}" LabelText="Installed Version:" d:LayoutOverrides="LeftPosition, RightPosition" Margin="{StaticResource DetailItemMargins}"/>
                                    <Controls:LabeledTextBlock LabelText="Latest Version:" Margin="{StaticResource DetailItemMargins}" BodyText="[Unbound]"/>
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="{StaticResource DetailItemMargins}" LabelText="Enable RCON Port:" InputText="{Binding RCONPort}" HasCheckBox="True" IsCheckboxChecked="{Binding RCONEnabled}"/>
                                    <Controls:LabeledTextBlock BodyText="{Binding Runtime.Status, FallbackValue=[Runtime Value]}" LabelText="Server Status:" Margin="{StaticResource DetailItemMargins}"/>
                                    <Controls:LabeledTextBlock BodyText="{Binding Runtime.Status, FallbackValue=[Runtime Value]}" LabelText="Availability:" Margin="{StaticResource DetailItemMargins}"/>
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="{StaticResource DetailItemMargins}" LabelText="Query Port:" InputText="{Binding Profile.ServerPort}"/>
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="{StaticResource DetailItemMargins}" LabelText="Server Port:" InputText="{Binding Profile.ServerConnectionPort}"/>
                                    <Controls:LabeledTextBlock LabelText="Local IP:" Margin="{StaticResource DetailItemMargins}"/>
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="{StaticResource DetailItemMargins}" LabelText="Enable Idle Timeout (min):" InputText="{Binding Profile.EnableKickIdlePlayers}" ToolTip="Enables automatically kicking players that have been idle for the time you choose." HasCheckBox="True"/>

                                    <local:LabeledSpinner Width="Auto" Height="Auto" Margin="{StaticResource DetailItemMargins}" LabelText="Auto Save Period (Minutes):" Value="{Binding Profile.AutoSavePeriodMinutes}" Minimum="1" Maximum="9999"/>
                                    <local:LabeledSlider Margin="4" IsChecked="True" LabelText="Max Players:" LargeChange="10" Maximum="127" Minimum="1" SmallChange="1"  Value="{Binding Profile.MaxPlayers}" Width="Auto" Height="Auto"/>
                                </UniformGrid>
                            </GroupBox>
                            <GroupBox Height="210" Margin="6,0,6,10" Header="Message of the Day:"  Foreground="{StaticResource TextLight_Brush}" FontSize="16" FontFamily="{StaticResource HeaderFont}">

                                <Grid Background="{StaticResource VeryDarkNeutral_Brush}" Margin="{StaticResource DetailItemMargins}" d:LayoutOverrides="LeftPosition, RightPosition, TopPosition, BottomPosition">
                                    <TextBox TextWrapping="Wrap" MaxLines="5" AcceptsReturn="True" Text="{Binding Profile.MOTD}" Style="{StaticResource DefaultTextBox}" FontFamily="{StaticResource DefaultTextBoxFont}" FontSize="{StaticResource DefaultTextBoxSize}" Margin="10,10,10,35"/>
                                    <eo:SpinEdit Value="{Binding Profile.MOTDDuration}" Margin="164,0,0,9" FontFamily="{StaticResource DetailBody_Font}" FontWeight="Bold" FontSize="{StaticResource DetailBodyFont_Size}" Maximum="{Binding Maximum}" Background="{StaticResource TextBoxBG_Brush}" BorderBrush="{StaticResource VeryDarkNeutral_Brush}" BorderThickness="0" Width="100" HorizontalAlignment="Left" LargeChange="5" Minimum="{Binding Minimum}" SmallChange="1" VerticalAlignment="Bottom"/>
                                    <TextBlock x:Name="textBlock_Copy" TextWrapping="Wrap" Foreground="{StaticResource TextLight_Brush}" FontFamily="{StaticResource DetailLabelFont}" FontSize="{StaticResource DetailLabelFont_Size}" FontWeight="Bold" Margin="10,0,0,10" VerticalAlignment="Bottom" HorizontalAlignment="Left" Text="Duration (Seconds):"/>
                                </Grid>
                            </GroupBox>
                            <GroupBox Header="Passwords" FontFamily="{StaticResource HeaderFont}" FontSize="16" Foreground="{StaticResource TextLight_Brush}" d:LayoutOverrides="LeftPosition, RightPosition" Margin="6,0,6,10">
                                <UniformGrid Rows="1" Columns="3" Height="70">
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="4,4,4,4" LabelText="Server Password:" InputText="{Binding Profile.ServerPassword}"/>
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="4,4,4,4" LabelText="Admin Password:" InputText="{Binding Profile.AdminPassword}"/>
                                    <local:LabeledTextBox Width="Auto" Height="Auto" Margin="4,4,4,4" LabelText="Spectator Password:" InputText="{Binding Profile.SpectatorPassword}"/>
                                </UniformGrid>
                            </GroupBox>
                            <GroupBox Header="Automatic Management" FontFamily="{StaticResource HeaderFont}" FontSize="16" Foreground="{StaticResource TextLight_Brush}" Margin="6,0,6,10" d:LayoutOverrides="LeftPosition, RightPosition">
                                <Grid Margin="12,12,0,12" HorizontalAlignment="Left" VerticalAlignment="Top" d:LayoutOverrides="TopPosition, BottomPosition">
                                    <Border BorderThickness="2" Margin="0,38,0,0" BorderBrush="{StaticResource Master_Brush}" Width="400.167"/>
                                    <StackPanel Orientation="Vertical" Margin="10,47,10,10" HorizontalAlignment="Left" IsEnabled="{Binding IsChecked, ElementName=checkBox1}">
                                        <CheckBox Height="22" IsChecked="{Binding Profile.EnableAutoStart}" Content="Auto-start Server On Boot" FontFamily="{StaticResource DetailBody_Font}" FontSize="{StaticResource DetailBodyFont_Size}" Foreground="{StaticResource TextLight_Brush}" Width="196.533" HorizontalAlignment="Left"/>
                                        <local:LabeledTextBox InputText="UNBOUND"  LabelText="Update Every (hh:mm):" Height="62" Width="375" HorizontalAlignment="Left" Margin="0,8,0,0" Style="{StaticResource LabeledTextBoxDefault}" />
                                        <local:LabeledTextBox InputText="UNBOUND"  LabelText="Restart Grace Period (min):" Height="62" Width="375" HorizontalAlignment="Left" Margin="0,8,0,0" Style="{StaticResource LabeledTextBoxDefault}" />
                                        <local:LabeledTextBox InputText="UNBOUND"  HasCheckBox="True" LabelText="Force Updates and Restart At (hh:mm):" Height="62" Width="375" HorizontalAlignment="Left" Margin="0,8,0,0" Style="{StaticResource LabeledTextBoxDefault}" />
                                    </StackPanel>
                                    <CheckBox x:Name="checkBox1" Height="22" Content="Enable Periodic Auto-Updates" IsChecked="{Binding Profile.EnableAutoUpdate}" FontFamily="{StaticResource DetailBody_Font}" FontSize="{StaticResource DetailBodyFont_Size}" Foreground="{StaticResource TextLight_Brush}" VerticalAlignment="Top" HorizontalAlignment="Left" Width="375" d:LayoutOverrides="VerticalAlignment" Margin="10,11,0,0"/>
                                </Grid>
                            </GroupBox>
                        </StackPanel>
                    </ScrollViewer>
